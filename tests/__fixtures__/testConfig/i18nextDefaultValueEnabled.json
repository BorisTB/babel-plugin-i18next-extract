{
  "inputFiles": ["i18nextDefaultValue.js"],
  "description": "test that default i18next default value is set for all locales when true",
  "pluginOptions": {
    "locales": ["en", "de"],
    "useI18nextDefaultValue": true
  },
  "expectValues": [
    [{
      "key0": "a default value",
      "key1": "another default value"
    }, {"locale": "en"}],
    [{
      "key0": "a default value",
      "key1": "another default value"
    }, {"locale": "de"}]
  ]
}
